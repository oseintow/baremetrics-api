{
  "name": "oseintow/baremetrics-api",
  "description": "A package to easily work with baremetrics",
  "license": "MIT",
  "authors": [
    {
      "name": "michael osei ntow",
      "email": "oseintow@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "guzzlehttp/guzzle": "^6.2",
    "illuminate/support": "^5.4",
    "vlucas/phpdotenv": "^3.3"
  },
  "require-dev": {
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "Oseintow\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ],
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist"
  }
}
